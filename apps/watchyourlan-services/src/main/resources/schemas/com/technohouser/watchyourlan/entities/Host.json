{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "properties" : {
    "date" : {
      "type" : "string",
      "format" : "date-time"
    },
    "hw" : {
      "type" : "string"
    },
    "id" : {
      "type" : "integer",
      "format" : "int32"
    },
    "ip" : {
      "type" : "string",
      "pattern" : "^(?:[0-9]{1,3}\\.){3}[0-9]{1,3}$"
    },
    "known" : {
      "type" : "boolean"
    },
    "mac" : {
      "type" : "string",
      "pattern" : "^([0-9A-Fa-f]{2}[:-]){5}([0-9A-Fa-f]{2})$"
    },
    "name" : {
      "type" : "string",
      "minLength" : 1
    },
    "now" : {
      "type" : "boolean"
    }
  },
  "required" : [ "name" ]
}